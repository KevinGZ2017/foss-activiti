<Row type="flex" justify="center" align="middle">
    <Col span="24">
        <Form :model="model" :label-width="160">

            <!-- 流程发起表单内容 -->
            <div style="background:#eee;padding: 20px 20px 10px 20px">
                <Card :bordered="false">
                    <p>
                        <Form-item :label="startFormBo.label" v-for="startFormBo in startFormBos">
                            {{ startFormBo.value }}
                        </Form-item>
                    </p>
                </Card>
            </div>

            <!-- 已审批内容 -->
            <div style="background:#eee;padding: 10px 20px 10px 20px" v-for="taskBo in taskBos">
                <Card :bordered="false">
                    <p>
                        <Form-item label="审批人">
                            {{ taskBo.approveUserName }}
                        </Form-item>
                        <Form-item label="审批时间">
                            {{ taskBo.startTime }} - {{ taskBo.endTime }}
                        </Form-item>
                        <Form-item label="是否审批通过">
                            {{ taskBo.auditText }}
                        </Form-item>
                        <Form-item label="审批意见">
                            {{ taskBo.auditDesc }}
                        </Form-item>
                    </p>
                </Card>
            </div>

            <div style="background:#eee;padding: 10px 20px 20px 20px">
                <Card :bordered="false">
                    <p slot="title">流程审批</p>
                    <p>
                        <Form-item label="是否同意申请">
                            <Select v-model="model._isAudit" style="width:80px">
                                <Option value="true">同意</Option>
                                <Option value="false">驳回</Option>
                            </Select>
                        </Form-item>

                        <Form-item label="审批意见">
                            <Input v-model="model._auditDesc" placeholder="请输入" type="textarea" :rows="5"
                                   style="width: 300px;"></Input>
                        </Form-item>
                    </p>
                </Card>
            </div>
        </Form>
    </Col>
</Row>
